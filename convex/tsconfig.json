{
	"compilerOptions": {
		"allowJs": true,
		"strict": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"lib": ["ES2021", "dom", "ESNext.Array"],
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["./**/*"],
	"exclude": ["./_generated"]
}
